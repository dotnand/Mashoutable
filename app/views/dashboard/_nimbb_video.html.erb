            width="100" 
            height="100" 
            codebase="http://fpdownload.macromedia.com/get/flashplayer/current/swflash.cab"> 
    <param value="transparent" name="wmode"> 
    <param name="movie" 
           value="http://player.nimbb.com/nimbb.swf?guid=<%= video.guid %>"
           logoimageurl="<%= asset_path('logo-thumbnail.png') %>&logoclickurl=<%= root_url %>&key=b47ef629cb&lang=en"/> 
    <param name="allowScriptAccess" value="always"/> 
    <embed name="nimbb" 
           src="http://player.nimbb.com/nimbb.swf?guid=<%= video.guid %>&logoimageurl=<%= asset_path('logo-thumbnail.png') %>&logoclickurl=<%= root_url %>&key=b47ef629cb&lang=en"   
           width="100" 
           height="100" 
           allowScriptAccess="always" 
           pluginspage="http://www.adobe.com/go/getflashplayer"></embed>
    </object>
  </div>
  <div class='span-6 last'>
      <h4>Name of Video</h4>
      <input value="private" type="checkbox">
      <label>Make Private</label>
      <br>
      <a href="#">Delete</a>
  </div>
</div>
